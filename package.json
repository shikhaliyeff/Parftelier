{
  "name": "perfume-recommender",
  "version": "1.0.0",
  "description": "AI-powered perfume recommendation app with Fragrantica data",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "scraper": "node scraper/index.js",
    "db:setup": "node scripts/setup-database.js",
    "db:seed": "node scripts/seed-data.js",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.1",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.2",
    "axios": "^1.6.0",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "perfume",
    "recommendation",
    "ai",
    "fragrance",
    "react",
    "nodejs"
  ],
  "author": "Perfume Recommender Team",
  "license": "MIT"
}
